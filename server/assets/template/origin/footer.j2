{% load static %}
<footer
        class="site-footer site-footer__3 pt-115 position-relative bg_img"
        data-opacity="8"
        data-overlay="dark"
        data-background="{% static '/' %}images/bg/footer-3-1.jpg">
        <!-- blog area start -->
    <div class="blog__area position-relative pb-120">
        <div class="container">
            <div class="row align-items-end mb-60">
                <div class="col-lg-12">
                    <div class="section-heading text-center">
                        <span class="sub-title">From our blog</span>
                        <h2 class="title">Our News & Articles</h2>
                    </div>
                </div>
            </div>
            <div class="row mt-none-30">
                <div class="col-xl-4 col-lg-6 col-md-6 mt-30">
                    <article class="blog__item">
                        <div class="thumb p-0 position-relative">
                            <div class="img">
                                <img src="{% static '/' %}images/blog/blog-1.jpg" alt="" />
                            </div>
                        </div>
                        <div class="content">
                            <ul
                                class="blog__meta list-unstyled d-flex align-items-center justify-content-start"
                            >
                                <li><span>By: </span> Rasalina</li>
                                <li>\</li>
                                <li>Life</li>
                                <li>\</li>
                                <li>23.01.2017</li>
                            </ul>
                            <h3 class="blog__title border-effect">
                                <a href="blog-details.html"
                                    >ideal cocktails from our barmen for pefect mood</a
                                >
                            </h3>
                            <a href="blog-details.html" class="read-more mt-20"
                                >Read more <i class="fa fa-long-arrow-right"></i
                            ></a>
                        </div>
                    </article>
                </div>
                <div class="col-xl-4 col-lg-6 col-md-6 mt-30">
                    <article class="blog__item">
                        <div class="thumb p-0 position-relative">
                            <div class="img">
                                <img src="{% static '/' %}images/blog/blog-2.jpg" alt="" />
                            </div>
                        </div>
                        <div class="content">
                            <ul
                                class="blog__meta list-unstyled d-flex align-items-center justify-content-start"
                            >
                                <li><span>By: </span> Rasalina</li>
                                <li>\</li>
                                <li>Life</li>
                                <li>\</li>
                                <li>23.01.2017</li>
                            </ul>
                            <h3 class="blog__title border-effect">
                                <a href="blog-details.html"
                                    >Build a cool mornig with cafena Coffee</a
                                >
                            </h3>
                            <a href="blog-details.html" class="read-more mt-20"
                                >Read more <i class="fa fa-long-arrow-right"></i
                            ></a>
                        </div>
                    </article>
                </div>
                <div class="col-xl-4 col-lg-6 col-md-6 mt-30">
                    <article class="blog__item">
                        <div class="thumb p-0 position-relative">
                            <div class="img">
                                <img src="{% static '/' %}images/blog/blog-3.jpg" alt="" />
                            </div>
                        </div>
                        <div class="content">
                            <ul
                                class="blog__meta list-unstyled d-flex align-items-center justify-content-start"
                            >
                                <li><span>By: </span> Rasalina</li>
                                <li>\</li>
                                <li>Life</li>
                                <li>\</li>
                                <li>23.01.2017</li>
                            </ul>
                            <h3 class="blog__title border-effect">
                                <a href="blog-details.html"
                                    >Questions business must be able to answers</a
                                >
                            </h3>
                            <a href="blog-details.html" class="read-more mt-20"
                                >Read more <i class="fa fa-long-arrow-right"></i
                            ></a>
                        </div>
                    </article>
                </div>
            </div>
        </div>
    </div>
    <!-- blog area end -->
    <div class="footer__top footer__top--3 pt-155 position-relative">
        <a
            href="#"
            class="go-top position-absolute d-flex align-items-center justify-content-center"
        >
            <i class="fal fa-long-arrow-up"></i>
            <span>go to top</span>
        </a>
        <div class="container">
            <div class="row justify-content-center mt-none-30">
                <div class="col-xl-3 col-lg-6 mt-30">
                    <div class="footer__widget footer__info">
                        <div
                            class="item d-flex align-items-center justify-content-center"
                        >
                            <img src="{% static '/' %}images/icons/ft-phone.png" alt="" />
                            <span>Phone :</span>
                            <a href="tel:88019339702520">+88019 339 702 520</a>
                        </div>
                    </div>
                </div>
                <div class="col-xl-4 col-lg-6 mt-30 pl-55">
                    <div class="footer__widget footer__info">
                        <div
                            class="item d-flex align-items-center justify-content-start"
                        >
                            <img src="{% static '/' %}images/icons/ft-map-marker.png" alt="" />
                            <span>Address :</span>
                            <p>30 North West New</p>
                        </div>
                    </div>
                </div>
                <div class="col-xl-5 col-lg-8 mt-30 pl-55">
                    <div class="footer__widget">
                        <div class="subscribe">
                            <form
                                action="https://xpressrow.com/html/cafena/cafena/index.html"
                            >
                                <input
                                    type="email"
                                    name="email"
                                    id="email"
                                    placeholder="Enter your email"
                                />
                                <button type="submit" class="subscribe__btn">
                                    Subscribe <i class="fa fa-paper-plane"></i>
                                </button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="footer__middle mt-65">
        <div class="container">
            <div class="row">
                <div class="col-lg-7">
                    <div class="footer__logo--content">
                        <img style="height:100px;object-fit:contain" class="mb-15" src="{% static '/' %}images/logo/logo.png" alt="" />
                        <p>
                            A yawn is a silent scream..... for coffee.
                        </p>
                    </div>
                </div>
                <div class="col-lg-5 align-self-end">
                    <div
                        class="social-links d-flex align-items-center justify-content-lg-end"
                    >
                        <a href="#0" target="_blank"><i class="fab fa-twitter"></i></a>
                        <a href="#0" target="_blank"
                            ><i class="fab fa-facebook-f"></i
                        ></a>
                        <a href="#0" target="_blank"><i class="fab fa-youtube"></i></a>
                        <a href="#0" target="_blank"
                            ><i class="fab fa-google-plus-g"></i
                        ></a>
                        <a href="#0" target="_blank"
                            ><i class="fab fa-instagram"></i
                        ></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="footer__menu-area mt-30 pb-90">
        <div class="container">
            <div class="row">
                <div class="col-lg-9">
                    <div class="footer__menu">
                        <nav>
                            <ul>
                                {% with request.resolver_match.url_name as url_name %}
                                    <li class="{% if url_name == 'home' %}active{% endif %}"><a href="{% url 'shop:home' %}">Home page</a></li>
                                    <li class="{% if url_name == 'shop' %}active{% endif %}"><a href="#">shop</a></li>
                                    <li class="{% if url_name == 'blog' %}active{% endif %}"><a href="#">blog</a></li>
                                    <li class="{% if url_name == 'contact' %}active{% endif %}"><a href="#">contact</a></li>
                                {% endwith %}
                            </ul>
                        </nav>
                    </div>
                </div>
                <div class="col-lg-3 text-center text-lg-end">
                    <a href="#" class="go-top go-top__bottom">
                        <span>go top</span>
                        <i class="fal fa-long-arrow-up"></i>
                    </a>
                </div>
            </div>
        </div>
    </div>
</footer>